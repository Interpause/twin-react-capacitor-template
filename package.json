{
  "name": "twin-react-capacitor-template",
  "version": "1.0.0",
  "description": "template for capacitor app using react framework with twin.macro and typescript",
  "main": "index.tsx",
  "scripts": {
    "dev": "cross-env NODE_ENV=development wp",
    "prod": "cross-env NODE_ENV=production wp",
    "gen-res": "cordova-res android --resources ./assets/res --skip-config --fit contain --copy",
    "build": "run-s prod && cap sync",
    "android": "run-s build gen-res && cap run android"
  },
  "keywords": [
    "capacitor",
    "twin.macro",
    "react",
    "typescript",
    "webpack",
    "react-refresh"
  ],
  "author": "John-Henry Lim <hyphen@interpause.dev> (https://interpause.dev/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.15.0",
    "@capacitor/cli": "^3.2.0",
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/babel-plugin-jsx-pragmatic": "^0.1.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cordova-res": "^0.15.3",
    "cross-env": "^7.0.3",
    "html-webpack-plugin": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "react-refresh": "^0.10.0",
    "react-refresh-typescript": "^2.0.2",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "webpack": "^5.51.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-nano": "^1.1.1",
    "webpack-plugin-serve": "^1.5.0"
  },
  "dependencies": {
    "@capacitor/android": "^3.2.0",
    "@capacitor/core": "^3.2.0",
    "@capacitor/device": "^1.0.2",
    "@capacitor/splash-screen": "^1.1.1",
    "@capacitor/storage": "^1.0.3",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "core-js": "^3.16.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tslib": "^2.3.1",
    "twin.macro": "^2.6.2"
  },
  "browserslist": "chrome 60"
}
